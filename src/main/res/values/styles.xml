    <style name="AppThemeLight.Colored">
        <item name="toolbar_content_color">@color/white</item>
        <item name="them_status_bar_is_light">false</item>
        <item name="toolbar_content_color_secondary">@color/grey_700</item>
        <item name="colorPrimaryDark">@color/blue_700</item>
        <item name="colorAccent">@color/blue_a_400</item>
        <item name="colorPrimary">@color/blue_700</item>
        <item name="android:navigationBarColor">?colorPrimary</item>
        <item name="navigation_bar_is_light">false</item>
        <item name="revers_color">@color/white</item>
        <item name="revers_color_lighted">@color/grey_200</item>
    </style>


    <style name="DialogSheetAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up_dialog</item>
        <item name="android:windowExitAnimation">@anim/slide_out_down</item>
    </style>

    <style name="Touchable">
        <item name="android:background">?selectableItemBackground</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="TouchableForeground">
        <item name="android:foreground">?selectableItemBackground</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="Icon">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:padding">12dp</item>
    </style>

    <style name="Icon.Shadow">
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="ViewIcon_background">@color/focus_dark</item>
        <item name="android:layout_gravity">right|top</item>
    </style>

    <style name="Icon.Shadow.Filtered">
        <item name="ViewIcon_filter">?toolbar_content_color</item>
    </style>

    <style name="Icon.Shadow.Back">
        <item name="android:id">@id/vBack</item>
        <item name="android:src">?attr/ic_arrow_back_24dp</item>
        <item name="android:layout_gravity">left|top</item>
    </style>

    <style name="Icon.Shadow.Back.Filtered">
        <item name="ViewIcon_filter">?toolbar_content_color</item>
    </style>

    <style name="Icon.Toolbar">
        <item name="android:layout_gravity">right</item>
    </style>

    <style name="Icon.Toolbar.Shadow">
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="ViewIcon_background">@color/focus_dark</item>
    </style>

    <style name="Icon.Toolbar.Shadow.Back">
        <item name="android:id">@id/vBack</item>
        <item name="android:src">?attr/ic_arrow_back_24dp</item>
    </style>

    <style name="Icon.Toolbar.Shadow.Back.Filtered">
        <item name="ViewIcon_filter">?toolbar_content_color</item>
    </style>

    <style name="Icon.Toolbar.Shadow.Filtered">
        <item name="ViewIcon_filter">?toolbar_content_color</item>
    </style>

    <style name="Icon.Toolbar.Filtered">
        <item name="ViewIcon_filter">?toolbar_content_color</item>
    </style>

    <style name="Icon.48">
        <item name="android:layout_width">72dp</item>
        <item name="android:layout_height">72dp</item>
        <item name="android:padding">18dp</item>
    </style>

    <style name="Icon.18">
        <item name="android:layout_width">32dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:padding">8dp</item>
    </style>

    <style name="Icon.12">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:padding">6dp</item>
    </style>

    <style name="Root">
        <item name="android:id">@id/vScreenRoot</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?android:windowBackground</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Root.Content">
        <item name="android:background">?content_background_screen</item>
    </style>

    <style name="Toolbar">
        <item name="android:id">@id/vToolbar</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorPrimary">?attr/toolbar_content_color</item>
        <item name="android:textColorSecondary">?attr/toolbar_content_color</item>
        <item name="actionMenuTextColor">?attr/toolbar_content_color</item>
        <item name="layout_scrollFlags">scroll|enterAlways|snap</item>
    </style>

    <style name="Toolbar.Collapse">
        <item name="layout_collapseMode">pin</item>
        <item name="android:background">@null</item>
    </style>

    <style name="Toolbar.NoScroll">
        <item name="layout_scrollFlags"></item>
    </style>

    <style name="AppBar">
        <item name="android:id">@id/vAppBar</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="layout_behavior">com.sup.dev.android.views.support.AppBarLayoutBehavior</item>
    </style>

    <style name="AppBar.Transparent">
        <item name="android:elevation">0dp</item>
        <item name="android:background">@null</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="CollapsingToolbar">
        <item name="android:id">@id/vCollapsingToolbar</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="expandedTitleMarginEnd">64dp</item>
        <item name="expandedTitleMarginStart">48dp</item>
        <item name="contentScrim">?colorPrimary</item>
        <item name="layout_scrollFlags">scroll|enterAlways|enterAlwaysCollapsed</item>
    </style>

    <style name="Fab">
        <item name="android:id">@id/vFab</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">right|bottom</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginBottom">16dp</item>
    </style>

    <style name="Fab.Mini">
        <item name="fabSize">mini</item>
    </style>

    <style name="Widget.GifView" parent="@android:style/Widget" />

    <style name="Divider">
        <item name="android:id">@id/vDivider</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?color_divider</item>
        <item name="android:layout_gravity">bottom</item>
    </style>

    <style name="Divider.8">
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>

    <style name="Divider.16">
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginLeft">16dp</item>
    </style>

    <style name="Divider.24">
        <item name="android:layout_marginRight">24dp</item>
        <item name="android:layout_marginLeft">24dp</item>
    </style>

    <style name="Recycler">
        <item name="android:id">@id/vRecycler</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="layout_behavior">@string/appbar_scrolling_view_behavior</item>
    </style>

    <style name="Recycler.Animated">
        <item name="android:layoutAnimation">@anim/layout_animation_slide_from_bottom</item>
    </style>

    <style name="Pager">
        <item name="android:id">@id/vPager</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="layout_behavior">@string/appbar_scrolling_view_behavior</item>
    </style>

    <style name="Scroll">
        <item name="android:id">@id/vScroll</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="layout_behavior">@string/appbar_scrolling_view_behavior</item>
    </style>

    <style name="Scroll.Widget">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="SwipeRefresh">
        <item name="android:id">@id/vRefresh</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="layout_behavior">@string/appbar_scrolling_view_behavior</item>
    </style>

    <style name="ChipMini">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:background">?colorAccent</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Chip">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">32dp</item>
        <item name="rippleColor">?colorAccent</item>
        <item name="chipBackgroundColor">?selector_chip</item>
        <item name="chipStrokeColor">?color_stroke</item>
        <item name="chipStrokeWidth">0dp</item>
        <item name="chipStartPadding">0dp</item>
        <item name="chipEndPadding">0dp</item>
        <item name="textStartPadding">12dp</item>
        <item name="textEndPadding">12dp</item>
        <item name="iconEndPadding">0dp</item>
    </style>

    <style name="Chip.Mini">
        <item name="android:layout_height">22dp</item>
        <item name="textStartPadding">8dp</item>
        <item name="textEndPadding">8dp</item>
    </style>

    <style name="Chip.Outline">
        <item name="chipStrokeWidth">1dp</item>
        <item name="chipBackgroundColor">?selector_chip_outline</item>
    </style>

    <style name="Chip.Choice">
        <item name="android:checkable">true</item>
        <item name="android:clickable">true</item>
        <item name="checkedIconVisible">false</item>
        <item name="closeIconVisible">false</item>
        <item name="checkedIcon">@drawable/ic_mtrl_chip_checked_black</item>
    </style>

    <style name="Chip.Choice.Outline">
        <item name="chipStrokeWidth">1dp</item>
        <item name="chipBackgroundColor">?selector_chip_outline</item>
    </style>

    <style name="Chip.Filter">
        <item name="android:checkable">true</item>
        <item name="closeIconVisible">false</item>
        <item name="checkedIcon">@drawable/ic_mtrl_chip_checked_black</item>
    </style>

    <style name="Chip.Filter.Outline">
        <item name="chipStrokeWidth">1dp</item>
        <item name="chipBackgroundColor">?selector_chip_outline</item>
    </style>

    <style name="ChipGroup">
        <item name="android:id">@id/vChipGroup</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="chipSpacing">8dp</item>
    </style>

    <style name="Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Button.Text" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Button.Outlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="strokeColor">?color_stroke</item>
    </style>

    <style name="TextInputLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TextInputLayout.Outlined" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="boxBackgroundColor">@color/focus</item>
    </style>

    <style name="Field">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

</resources>